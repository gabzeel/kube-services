{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/kube-services/src",
  "monorepo": true,
  "root": "apps/kube-services",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/kube-services/tsconfig.app.json"
  },
  "projects": {
    "kube-services": {
      "type": "application",
      "root": "apps/kube-services",
      "entryFile": "main",
      "sourceRoot": "apps/kube-services/src",
      "compilerOptions": {
        "tsConfigPath": "apps/kube-services/tsconfig.app.json"
      }
    },
    "my-app": {
      "type": "application",
      "root": "apps/my-app",
      "entryFile": "main",
      "sourceRoot": "apps/my-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/my-app/tsconfig.app.json"
      }
    },
    "mqtt": {
      "type": "library",
      "root": "libs/mqtt",
      "entryFile": "index",
      "sourceRoot": "libs/mqtt/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mqtt/tsconfig.lib.json"
      }
    }
  }
}