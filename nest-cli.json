{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/app/src",
  "monorepo": true,
  "root": "apps/app",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/app/tsconfig.app.json"
  },
  "projects": {
    "app": {
      "type": "application",
      "root": "apps/app",
      "entryFile": "main",
      "sourceRoot": "apps/app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app/tsconfig.app.json"
      }
    },
    "amqp-service": {
      "type": "application",
      "root": "apps/amqp-service",
      "entryFile": "main",
      "sourceRoot": "apps/amqp-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/amqp-service/tsconfig.app.json"
      }
    },
    "mqtt-service": {
      "type": "application",
      "root": "apps/mqtt-service",
      "entryFile": "main",
      "sourceRoot": "apps/mqtt-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mqtt-service/tsconfig.app.json"
      }
    },
    "redis-service": {
      "type": "application",
      "root": "apps/redis-service",
      "entryFile": "main",
      "sourceRoot": "apps/redis-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/redis-service/tsconfig.app.json"
      }
    },
    "mqtt": {
      "type": "library",
      "root": "libs/mqtt",
      "entryFile": "index",
      "sourceRoot": "libs/mqtt/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mqtt/tsconfig.lib.json"
      }
    },
    "test": {
      "type": "library",
      "root": "libs/test",
      "entryFile": "index",
      "sourceRoot": "libs/test/src",
      "compilerOptions": {
        "tsConfigPath": "libs/test/tsconfig.lib.json"
      }
    }
  }
}